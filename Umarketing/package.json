{
  "name": "umarketing",
  "version": "1.0.0",
  "description": "Test module for UM",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
